{
  "expo": {
    "name": "SeQre App",
    "slug": "seqre-app",
    "plugins": ["@react-native-google-signin/google-signin"],
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/iconMain.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {

      "adaptiveIcon": {
        "foregroundImage": "./assets/iconMain.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e280632e-8a04-484f-a3ee-89224b970aae"
      }
    },
